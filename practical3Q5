#include <stdio.h>
int main() {
    
    int num1,num2;
    //input
    printf("Enter two integers:");
    scanf("%d%d",&num1,&num2);
    //process
    if (num1%num2==0)
    printf("first number is a multiple of second number");
    else
    printf("first number is not a multiple of second number");

    return 0;
    
    
