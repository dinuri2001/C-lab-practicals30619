#include <stdio.h>
int main (){


char ch;
printf("Enter a character: ");
    scanf("%c",&ch);
    switch(ch) 
    {
        case 'a':
            printf("The character is a vowel\n");
        break;
            
        case 'e':
            printf(" The character is a vowel\n");
            break;
        case 'i':
        printf("The character is a vowel\n");
        break;
        case 'o':
        printf("The character is a vowel\n");
        break;
        case 'u':
        printf("The character is a vowel\n");
        break;
      
        default: printf("%c is not a vowel:\n",ch);
    }
    return 0;
}
