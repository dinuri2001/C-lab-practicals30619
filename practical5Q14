#include <stdio.h>
int main(){
    int i, numarr[10] ,evenc=0,oddc=0;
    for (i=0; i<10;i++){
    printf("Enter an integer:",i+1);
    scanf("%d",&numarr[i]);
    }
    
    for (i=1;i<=10;i++)
    {
        if (numarr[i]%2==0)
        evenc++;
        
         else 
         oddc++;
         
    }
     printf("The no.of even numbers entered are %d",evenc);
        
    printf("The no.of odd numbers entered are %d",oddc);
    
return 0;
}
