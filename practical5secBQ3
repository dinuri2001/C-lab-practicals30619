#include <stdio.h>
int main (){
    int i,prices[10],sum=0,count=0;
    float avg;
     for (i=0;i<=10;i++)
     {
         printf("Enter the price of 10 item:",i+1);
         scanf("%d",& prices[i]);
     }
    

    for (i=1;i<=10;i++) {
        sum=sum+prices[i];
    

    if (prices[i]>200){
        count++;
    }
    }
    avg=sum/10;
    printf("The average value of an item is %f",avg);
    printf("The no.of items greater than 200 are %d",count);
    return 0;
    
}
